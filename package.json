{
  "name": "systemtests-sau-portal",
  "version": "1.0.0",
  "description": "Automated system tests for sau-portal.de using Playwright with GitHub Copilot agent integration",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:codegen": "playwright codegen https://sau-portal.de",
    "report": "playwright show-report",
    "install:browsers": "playwright install"
  },
  "keywords": [
    "playwright",
    "testing",
    "e2e",
    "automation",
    "copilot-agents"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.9.1",
    "typescript": "^5.9.3"
  }
}
